{
  "name": "bluetifulplanet-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:turbo": "next dev --turbo",
    "test": "env-cmd -f .env.local jest --watchAll=false --transformIgnorePatterns \"node_modules/(?!swiper)/\"",
    "test:coverage": "env-cmd -f .env.local jest --coverage --transformIgnorePatterns \"node_modules/(?!swiper)/\"",
    "compile": "env-cmd -f .env.local graphql-codegen",
    "watch": "env-cmd -f .env.local graphql-codegen -w",
    "prepare": "husky install",
    "export": "next export"
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/node": "18.11.18",
    "@types/react-dom": "18.0.10",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "next": "^13.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swiper": "^8.4.6",
    "typescript": "^5.1.3",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.4",
    "@graphql-codegen/client-preset": "^1.2.6",
    "@testing-library/dom": "^8.19.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.2.12",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "autoprefixer": "^10.4.13",
    "env-cmd": "^10.1.0",
    "eslint": "8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.4.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-risxss": "^2.1.0",
    "eslint-plugin-security": "^1.6.0",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "eslint-plugin-sonarjs": "^0.18.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.3",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1"
  }
